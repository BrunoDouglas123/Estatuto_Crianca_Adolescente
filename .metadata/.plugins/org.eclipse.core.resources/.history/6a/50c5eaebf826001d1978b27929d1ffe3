package Teste.com.teste.DTO;

import Teste.com.teste.Model.Acao;

public class AcaoDTO {
	
	private Long id;
	private String nome;
	
	public AcaoDTO() {		
	}
	
	public AcaoDTO(Acao entity) {
		this.id = entity.getId();
		this.nome = entity.getNome();
	}
	
	public AcaoDTO(Long id, String nome) {
		this.id = id;
		this.nome = nome;
	}
	
	

}
